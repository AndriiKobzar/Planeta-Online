@model Planeta_Online.Models.VisitorsViewModel
@{
    ViewBag.Title = "Visitors";
}

<h2>Заявки на @Model.EventName</h2>
<table class="table">
    <tr>
        <th>N</th>
        <th>Ім'я</th>
        <th>Email</th>
    </tr>
   @for(int i=0; i<Model.Visitors.Count; i++)
    {       
        <tr>
            <td>@(i+1))</td>
            <td>@Html.DisplayFor(model=>model.Visitors[i].Name)</td>
            <td>@Html.DisplayFor(model => model.Visitors[i].Email)</td>
        </tr>
    }
</table>